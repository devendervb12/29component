<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		 xmlns:html="http://www.w3.org/1999/xhtml"
		 controllerName="smax.batch29.A2.controller.Page1"
		 >
	<Page title="Product List">
		<content>
	           <List items="{/ProductSet}">
	              <items>
	                 <StandardListItem title="{ProductID}" type="Active"
	                 press="onItemSelection"
	                 />
	              </items>
	           </List>
		</content>
	</Page>
</core:View>